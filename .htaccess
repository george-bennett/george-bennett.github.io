RewriteEngine on

RewriteEngine on
RewriteCond %{HTTP_HOST} ^george-bennett\.co.uk$
RewriteRule (.*) https://www.george-bennett.co.uk/$1 [R=301,L]
RewriteRule ^$ home [L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]

RewriteRule ^/?$ /home [L]